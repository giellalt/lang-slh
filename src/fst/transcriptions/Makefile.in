#! @param GT_TRANSCRIPTIONS_SRCS required, the source files for orthographic
#!        transcriptions.
#GT_TRANSCRIPTIONS_SRCS=clock.lexc date.lexc numbers.lexc
GT_TRANSCRIPTIONS_SRCS = \
		transcriptor-clock-digit2text.lexc \
		transcriptor-date-digit2text.lexc \
		transcriptor-numbers-digit2text.lexc

#! @param GT_ABBREV_SRCS required, for abbreviation expansions
GT_ABBREV_SRCS = transcriptor-abbrevs2text.lexc
GT_TRANSCRIPTIONS_REGEX_SRCS = 
GT_CUSTOM_TARGETS = 
EXTRA_DIST = $(GT_TRANSCRIPTIONS_SRCS) $(GT_TRANSCRIPTIONS_REGEX_SRCS) $(GT_ABBREV_SRCS)

# The transducers we build and don't distribute depend on the configuration:
GT_TRANSCRIPTORS = $(am__append_1) $(am__append_2) $(am__append_3)

# Replace .lexc with .filtered.lookup:
@WANT_TRANSCRIPTORS_TRUE@DIGIT2TXT = $(patsubst %.lexc,%.filtered.lookup,$(GT_TRANSCRIPTIONS_SRCS))
@WANT_TRANSCRIPTORS_TRUE@REGEX2TXT = $(patsubst %.regex,%.filtered.lookup,$(GT_TRANSCRIPTIONS_REGEX_SRCS))
@WANT_TRANSCRIPTORS_TRUE@ABBREV2TXT = $(patsubst %.lexc,%.filtered.lookup,$(GT_ABBREV_SRCS))
# Replace digit2text with text2digit to get the inverse fst's:
@WANT_TRANSCRIPTORS_TRUE@TXT2DIGIT = $(subst digit2text,text2digit,$(DIGIT2TXT))
# Rename arbitrary files ending in -XXX2text to -text2XXX:
@WANT_TRANSCRIPTORS_TRUE@TXT2ABBREV = $(shell for file in $(ABBREV2TXT) ; do basename $$file .filtered.lookup \
@WANT_TRANSCRIPTORS_TRUE@| cut -d'-' -f2 | sed 's/2text//' | sed 's/^/transcriptor-text2/' \
@WANT_TRANSCRIPTORS_TRUE@| sed 's/$$/.filtered.lookup/'; done )

noinst_DATA = $(GT_TRANSCRIPTORS) $(GT_CUSTOM_TARGETS)
@WITH_FOMA_TRUE@HFST_FORMAT = --format=foma
@WITH_OFST_LOG_TRUE@HFST_FORMAT = --format=openfst-log
@WITH_OFST_TROPICAL_TRUE@HFST_FORMAT = --format=openfst-tropical
@WITH_SFST_TRUE@HFST_FORMAT = --format=sfst
@WITH_FOMA_TRUE@HFST_OLFORMAT = --format=optimized-lookup-unweighted
@WITH_OFST_LOG_TRUE@HFST_OLFORMAT = --format=optimized-lookup-weighted
@WITH_OFST_TROPICAL_TRUE@HFST_OLFORMAT = --format=optimized-lookup-weighted
@WITH_SFST_TRUE@HFST_OLFORMAT = --format=optimized-lookup-unweighted

